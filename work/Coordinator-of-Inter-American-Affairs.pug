                    if dates == 'CHANGE_ME'
                    else
                        p!= dates

                    //- /*==============================  PROJECTS  ==============================*/
                    +galleryItem-blockGrid(
                        "DIVIDER_yes",
                        "Brochure",
                        "top","2","2",
                        {SM: "https://assets.paulrand.design/Works/Coordinator%20of%20Inter%20American%20Affairs/Web/Coordinator%20of%20Inter%20American%20Affairs%2C%20brochure%201943%2001-800.jpg", LG:"https://assets.paulrand.design/Works/Coordinator%20of%20Inter%20American%20Affairs/Web/Coordinator%20of%20Inter%20American%20Affairs%2C%20brochure%201943%2001.jpg", title: "", footnote: ""},
                        {SM: "https://assets.paulrand.design/Works/Coordinator%20of%20Inter%20American%20Affairs/Web/Coordinator%20of%20Inter%20American%20Affairs%2C%20brochure%201943%2002-800.jpg", LG:"https://assets.paulrand.design/Works/Coordinator%20of%20Inter%20American%20Affairs/Web/Coordinator%20of%20Inter%20American%20Affairs%2C%20brochure%201943%2002.jpg", title: "", footnote: ""}
                        )

            //- /*-----  End of BODY  ------*/


        include ../__MIXINS/_include-next-work.pug

        include ../__MIXINS/_include-social-media.pug