include ../__MIXINS/_mixins.pug

block vars
    - var pageTitle = 'IBM'
    - var bodyID    = 'work'
    - var bodyClass = ''

block main-content
    - var dates     = 'CHANGE_ME'
    - var nextType  = 'Client'
    - var nextTitle = 'ideo Design'
    - var nextLink  = 'ideo-Design'


