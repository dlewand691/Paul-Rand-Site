    .cd-main-content
        .grid-container
            .grid-x.grid-margin-x.align-center
                .small-12.cell.text-center
                    //- /*==============================  CLIENT  ==============================*/
                    h1.text-center.small-mb1.medium-mb2.reveal-enter-bottom!= pageTitle
                    if dates == 'CHANGE_ME'
                    else
                        p!= dates

            .grid-x.grid-margin-x.expanded.collapse#stickyNav
                .cell(data-sticky-container)
                    .sticky(style="width: 100%" data-sticky data-margin-top="" data-top-anchor="stickyNav")
                        ul.small.sans-serif.text-bold.vertical.medium-horizontal.menu.align-center.small-py-half.bg-gray-light3(style="flex-wrap: wrap;" data-magellan data-offset="225")
                            li
                                a.p-half(href="#logos") Logos
                            li
                                a.p-half(href="#annualReports") Annual Reports
                            li
                                a.p-half(href="#brandingGuidelines") Branding Guidelines
                            li
                                a.p-half(href="#posters") Posters
                            li
                                a.p-half(href="#packaging") Packaging
                            li
                                a.p-half(href="#collateral") Collateral
                            li
                                a.p-half(href="#rebus") Rebus
                            li
                                a.p-half(href="#galleryExhibits") Gallery Exhibits
                            li
                                a.p-half(href="#stationery") Stationery
                            li
                                a.p-half(href="#signage") Signage
                            li
                                a.p-half(href="#signage") Fabrics & Wallpapers
                            li
                                a.p-half(href="#stocks") Stocks
                            li
