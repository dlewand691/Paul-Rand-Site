extends ../__MIXINS/_layout-default.pug
include ../__MIXINS/_mixins.pug

block vars
    - var pageTitle = 'National Park Service'
    - var bodyID    = 'work'
    - var bodyClass = ''

block main-content
    - var dates     = 'CHANGE_ME'
    - var nextType  = 'Client'
    - var nextTitle = 'New York Advertising Co.'
    - var nextLink  = 'New-York-Advertising-Co'


    //- ==========  BODY  ==========
    .cd-main-content
        .grid-container
            .grid-x.grid-margin-x.align-center.small-mb4
                .small-12.cell.text-center
                    //- /*==============================  CLIENT  ==============================*/
                    h1.text-center.small-mb2.medium-mb3.large-mb3(data-aos="fade-up")!= pageTitle
                    if dates == 'CHANGE_ME'
                    else
                        p!= dates

                    //- /*==============================  PROJECTS  ==============================*/
                    +galleryItem-blockGrid(
                        "DIVIDER_yes",
                        "Boston National Historical Park",
                        "top","1","1",
                        {SM: "https://assets.paulrand.design/Works/National%20Park%20Service/Boston%20National%20Historical%20Park%20Poster/Web/Boston%20National%20Historical%20Park%20Poster%2001-970.jpg", LG:"https://assets.paulrand.design/Works/National%20Park%20Service/Boston%20National%20Historical%20Park%20Poster/Web/Boston%20National%20Historical%20Park%20Poster%2001.jpg", title: "", footnote: ""}
                        )

                    +galleryItem-blockGrid(
                        "DIVIDER_yes",
                        "Minute Man",
                        "top","1","1",
                        {SM: "https://assets.paulrand.design/Works/National%20Park%20Service/Minute%20Man/Web/Minute%20Man%2001-970.jpg", LG:"https://assets.paulrand.design/Works/National%20Park%20Service/Minute%20Man/Web/Minute%20Man%2001.jpg", title: "", footnote: ""}
                        )

            //- /*-----  End of BODY  ------*/


        include ../__MIXINS/_include-next-work.pug

        include ../__MIXINS/_include-social-media.pug