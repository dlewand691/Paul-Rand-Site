extends ../__MIXINS/_layout-default.pug
include ../__MIXINS/_mixins.pug

block vars
    - var pageTitle = 'International Design Conference, Aspen'
    - var bodyID    = 'work'
    - var bodyClass = ''

block main-content
    - var dates     = 'CHANGE_ME'
    - var nextType  = 'Client'
    - var nextTitle = 'Irwin-Sweeney-Miller Foundation'
    - var nextLink  = 'Irwin-Sweeney-Miller-Foundation'


    //- ==========  BODY  ==========
    .cd-main-content
        .grid-container
            .grid-x.grid-margin-x.align-center.small-mb4
                .small-12.cell.text-center
                    //- /*==============================  CLIENT  ==============================*/
                    h1.text-center.small-mb2.medium-mb3.large-mb3(data-aos="fade-up")!= pageTitle
                    if dates == 'CHANGE_ME'
                    else
                        p!= dates

                    //- /*==============================  PROJECTS  ==============================*/
                    +galleryItem-blockGrid(
                        "DIVIDER_yes",
                        "Poster, 1966",
                        "top","1","1",
                        {SM: "https://assets.paulrand.design/Works/International%20Design%20Conference-%20Aspen/Posters/Web/Aspen%20Design%20Conference%20Poster%2002a-970.jpg", LG:"https://assets.paulrand.design/Works/International%20Design%20Conference-%20Aspen/Posters/Web/Aspen%20Design%20Conference%20Poster%2002a.jpg", title: "", footnote: ""}
                        )

                    +galleryItem-blockGrid(
                        "DIVIDER_yes",
                        "Poster, 1982",
                        "top","1","1",
                        {SM: "https://assets.paulrand.design/Works/International%20Design%20Conference-%20Aspen/Posters/Web/Aspen%20Design%20Conference%20Poster%2001a-970.jpg", LG:"https://assets.paulrand.design/Works/International%20Design%20Conference-%20Aspen/Posters/Web/Aspen%20Design%20Conference%20Poster%2001a.jpg", title: "", footnote: ""}
                        )

                    +galleryItem-blockGrid(
                        "DIVIDER_yes",
                        "Brochure",
                        "top","3","2",
                        {SM: "https://assets.paulrand.design/Works/International%20Design%20Conference-%20Aspen/Brochure/Web/Aspen%20Design%20Conference%20Brochure%2001.jpg", LG:"https://assets.paulrand.design/Works/International%20Design%20Conference-%20Aspen/Brochure/Web/Aspen%20Design%20Conference%20Brochure%2001.jpg", title: "", footnote: ""},
