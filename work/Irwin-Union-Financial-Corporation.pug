                        {SM: "https://assets.paulrand.design/Works/Irwin%20Union%20Financial%20Corporation/Web/Irwin%20Union%20Corporation%20Annual%20Report%201992-800.jpg", LG:"https://assets.paulrand.design/Works/Irwin%20Union%20Financial%20Corporation/Web/Irwin%20Union%20Corporation%20Annual%20Report%201992.jpg", title: "", footnote: "1992"}
