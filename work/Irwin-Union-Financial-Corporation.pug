extends ../__MIXINS/_layout-default.pug
include ../__MIXINS/_mixins.pug

block vars
    - var pageTitle = 'Irwin Union Financial Corporation'
    - var bodyID    = 'work'
    - var bodyClass = ''

block main-content
    - var dates     = '1990'
    - var nextType  = 'Client'
    - var nextTitle = 'Isaac Goldman Company'
    - var nextLink  = 'Isaac-Goldman-Company'


    //- ==========  BODY  ==========
    .cd-main-content
        .grid-container
            .grid-x.grid-margin-x.align-center.small-mb4
                .small-12.cell.text-center
                    //- /*==============================  CLIENT  ==============================*/
                    h1.text-center.small-mb2.medium-mb3.large-mb3(data-aos="fade-up")!= pageTitle
                    if dates == 'CHANGE_ME'
                    else
                        p!= dates

                    //- /*==============================  PROJECTS  ==============================*/
                    +galleryItem-blockGrid(
                        "DIVIDER_yes",
                        "Logo",
                        "top","1","1",
                        {SM: "https://assets.paulrand.design/Works/Irwin%20Union%20Financial%20Corporation/Irwin-Financial-Corporation-1990.svg", LG:"https://assets.paulrand.design/Works/Irwin%20Union%20Financial%20Corporation/Irwin-Financial-Corporation-1990.svg", title: "", footnote: ""}
                        )

                    +galleryItem-blockGrid(
                        "DIVIDER_yes",
                        "Annual Reports",
                        "top","2","2",
                        {SM: "https://assets.paulrand.design/Works/Irwin%20Union%20Financial%20Corporation/Web/Irwin%20Union%20Corporation%20Annual%20Report%201969-800.jpg", LG:"https://assets.paulrand.design/Works/Irwin%20Union%20Financial%20Corporation/Web/Irwin%20Union%20Corporation%20Annual%20Report%201969.jpg", title: "", footnote: "1969"},
