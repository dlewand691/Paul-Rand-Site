{
  "env": {
    "browser": true,
    "es6": true,
    "jest/globals": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["jest"],
  "rules": {
    "quotes": ["error", "single"],
    "import/prefer-default-export": false,
    "comma-dangle": "off",
    "arrow-parens": ["error", "as-needed"],
    "consistent-return": "off"
  }
}
